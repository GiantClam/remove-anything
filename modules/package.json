{
  "name": "@giantclam/aiwebmodules",
  "version": "1.0.0",
  "description": "可复用的业务模块集合，包含 RunningHub、Tasks、Auth、Payments、Cloudflare Storage 等模块",
  "main": "./index.ts",
  "types": "./index.ts",
  "files": [
    "**/*.ts",
    "**/*.tsx",
    "!**/*.example.ts",
    "!examples/**",
    "!node_modules",
    "!dist"
  ],
  "scripts": {
    "type-check": "tsc --noEmit",
    "build": "tsc",
    "lint": "eslint . --ext .ts,.tsx",
    "prepublishOnly": "npm run type-check"
  },
  "keywords": [
    "modules",
    "runninghub",
    "tasks",
    "auth",
    "payments",
    "stripe",
    "cloudflare",
    "r2",
    "storage"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/GiantClam/aiwebmodules.git"
  },
  "bugs": {
    "url": "https://github.com/GiantClam/aiwebmodules/issues"
  },
  "homepage": "https://github.com/GiantClam/aiwebmodules#readme",
  "dependencies": {
    "aws-sdk": "^2.1692.0",
    "stripe": "^15.8.0"
  },
  "devDependencies": {
    "@types/node": "^22.3.0",
    "typescript": "^5.5.4"
  },
  "peerDependencies": {
    "@prisma/client": "^6.12.0"
  },
  "peerDependenciesMeta": {
    "@prisma/client": {
      "optional": true
    }
  }
}

